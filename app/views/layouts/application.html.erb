<!DOCTYPE html>
<html>
  <head>
    <title>SkillsTestProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class= "message" >  
      <% flash.each do |key, message| %>
        <p><%= message %></p>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
